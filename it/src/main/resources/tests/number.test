        { "nr": 6, "1": 9223372036854775808 },
        { "nr": 7, "1": 1.7976931348623157E308 },
        { "nr": 8, "1": -1.7976931348623157E308 },
        { "nr": 9, "1": 4.9E-324 },
        { "nr": 10, "1": 1E-400 },
        { "nr": 11 },
        { "nr": 12 },
        { "nr": 13 }
    ]
