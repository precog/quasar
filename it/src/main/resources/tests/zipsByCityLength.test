{
  "name": "count occurrences of each value of length(city), with filtering",
  "backends": { "mongodb_read_only": "pending" },

  "data": "zips.data",

  "query": "select length(city) as len, count(*) as cnt
    from zips
    where state != 'MI'
    group by length(city)",

  "predicate": "containsAtLeast",
  "expected": [
    { "len": 3.0, "cnt":  127 },
    { "len": 4.0, "cnt":  927 },
    { "len": 5.0, "cnt": 2181 }
  ]
}
