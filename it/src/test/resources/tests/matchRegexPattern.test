{
    "name": "match regex pattern",
    "backends": {"mongolabs": "verify"},
    "data": "zips.data",
    "query": "select city from zips where city ~ 'OULD.{0,2} CIT'",
    "expected": [{"city": "GOULD CITY"},
                 {"city": "BOULDER CITY"}],
    "ignoreOrder": true,
    "matchAll": true,
    "ignoredFields": ["_id"]
}
